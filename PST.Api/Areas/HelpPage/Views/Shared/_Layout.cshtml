<head>
	<title>GIE PST API Documentation</title>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width" />
	<script type="text/javascript" src="@Url.Content("~/Areas/HelpPage/scripts/jquery-1.10.2.min.js")"></script>
	@*<script type="text/javascript" src="@Url.Content("~/Areas/HelpPage/scripts/prettyPre.js")"></script>*@
	<script type="text/javascript" src="@Url.Content("~/Areas/HelpPage/scripts/jquery.toc.js")"></script>

	<link href="@Url.Content("~/Areas/HelpPage/images/favicon.ico")" rel="Shortcut Icon" />
	<link href="@Url.Content("~/Areas/HelpPage/css/normalize.css")" rel="stylesheet" type="text/css" media="all" />
	<link href="@Url.Content("~/Areas/HelpPage/css/base.css")" rel="stylesheet" type="text/css" media="all" />
	@RenderSection("scripts", required: false)

	<script>
		$(document).ready(function () {
			var offset = 220;
			var duration = 500;
			$(window).scroll(function () {
				if ($(this).scrollTop() > offset) {
					$('.back-to-top').fadeIn(duration);
				} else {
					$('.back-to-top').fadeOut(duration);
				}
			});

			$('.back-to-top').click(function(e) {
				e.stopImmediatePropagation();
				e.preventDefault();
				$('html, body').animate({ scrollTop: 0 }, duration);
			});
		});
	</script>
</head>

<body>
	<div id="main">
		@RenderBody()
	</div>
	<a href="#" class="back-to-top">Back to Top</a>
</body>
</html>